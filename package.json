{
  "name": "RRM-Starter",
  "version": "1.0.0",
  "description": "IQex crypto template",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js --colors --progress --profile",
    "test": "cross-env NODE_ENV=production jest",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "coverage": "cross-env NODE_ENV=development jest --coverage",
    "lint": "eslint .",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i"
  },
  "author": "Calin Ciupei",
  "license": "ISC",
  "dependencies": {
    "@material-ui/icons": "^3.0.2",
    "css-loader": "^2.1.0",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-classes": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.8.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^24.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "jest-cli": "^24.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.6.3",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sinon": "^7.2.3",
    "typeface-roboto": "^0.0.54",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-transform-classes",
        {
          "loose": true
        }
      ],
      "react-hot-loader/babel"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reducers/index.reducer.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/style/**",
      "!webpack.config.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": false,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ],
    "testEnvironment": "node"
  }
}
